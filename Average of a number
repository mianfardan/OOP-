mport java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner s = new Scanner(System.in);

        int a [] = new int[5];
        double result=0;

        for (int i=0; i<5; i++){
            result += a[i];
        }
        System.out.println("Average of input numbers = "+result/5);
    }
}
